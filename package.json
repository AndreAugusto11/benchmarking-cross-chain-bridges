{
  "name": "benchmarking-cross-chain-bridges",
  "version": "1.0.0",
  "description": "benchmarking aggregators",
  "main": "index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "run:hyperlane": "./src/message-aggregators/hyperlane/counter.sh 80001 5",
    "test:hyperlane": "./test/message-aggregators/hyperlane/counter.test.sh 80001 5",
    "test:cowswap": "mocha --require ts-node/register --require tsconfig-paths/register 'test/token-aggregators/cowswap/*.test.ts' --timeout 20000",
    "test:lifi": "mocha --require ts-node/register --require tsconfig-paths/register 'test/token-aggregators/lifi/*.test.ts' --timeout 20000",
    "test:socket": "mocha --require ts-node/register --require tsconfig-paths/register 'test/token-aggregators/socket/*.test.ts' --timeout 20000",
    "test:sushiswap": "mocha --require ts-node/register --require tsconfig-paths/register 'test/token-aggregators/sushiswap/*.test.ts' --timeout 20000",
    "test:uniswap": "mocha --require ts-node/register --require tsconfig-paths/register 'test/token-aggregators/uniswap/*.test.ts' --timeout 20000",
    "test:xy": "mocha --require ts-node/register --require tsconfig-paths/register 'test/token-aggregators/xy/*.test.ts' --timeout 20000",
    "test:helper": "mocha --require ts-node/register --require tsconfig-paths/register 'test/helper/*.test.ts' --timeout 20000",
    "test:token-aggregators": "mocha --require ts-node/register --require tsconfig-paths/register 'test/token-aggregators/**/*.test.ts' --timeout 20000",
    "test:message-aggregators": "./test/message-aggregators/**/*.test.sh",
    "test:contracts": "forge test",
    "test": "npm run test:token-aggregators && npm run test:message-aggregators",
    "clear:dry-run": "rm -rf broadcast/*/*/dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperledger-labs/benchmarking-cross-chain-bridges.git"
  },
  "author": "Shankar Subramanian",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hyperledger-labs/benchmarking-cross-chain-bridges/issues"
  },
  "homepage": "https://github.com/hyperledger-labs/benchmarking-cross-chain-bridges#readme",
  "dependencies": {
    "@lifi/sdk": "^2.2.2",
    "@sushiswap/sdk": "^5.0.0-canary.116",
    "@types/node": "^20.4.8",
    "@uniswap/sdk-core": "^4.0.6",
    "@uniswap/smart-order-router": "^3.15.0",
    "alchemy-sdk": "^2.9.2",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1"
  }
}