{
  "name": "benchmarking-cross-chain-bridges",
  "version": "1.0.0",
  "description": "benchmarking aggregators",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lifi:route": "npx ts-node src/lifi/route_builder.ts*",
    "socket:route": "npx ts-node src/socket/route_builder.ts*",
    "uniswap:route": "npx ts-node src/uniswap/route_builder.ts*",
    "test:lifi": "mocha --require ts-node/register 'test/lifi/*.test.ts' --timeout 20000",
    "test:socket": "mocha --require ts-node/register 'test/socket/*.test.ts' --timeout 20000",
    "test:uniswap": "mocha --require ts-node/register 'test/uniswap/*.test.ts' --timeout 20000",
    "test:helper": "mocha --require ts-node/register 'test/helper/*.test.ts' --timeout 20000",
    "test:all": "mocha --require ts-node/register 'test/**/*.test.ts' --timeout 20000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperledger-labs/benchmarking-cross-chain-bridges.git"
  },
  "author": "Shankar Subramanian",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hyperledger-labs/benchmarking-cross-chain-bridges/issues"
  },
  "homepage": "https://github.com/hyperledger-labs/benchmarking-cross-chain-bridges#readme",
  "dependencies": {
    "@lifi/sdk": "^2.2.2",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.8",
    "@uniswap/sdk-core": "^4.0.6",
    "@uniswap/smart-order-router": "^3.15.0",
    "@uniswap/v3-sdk": "^3.10.0",
    "alchemy-sdk": "^2.9.2",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "jest": "^29.6.2",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1"
  }
}